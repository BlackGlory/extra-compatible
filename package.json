{
  "name": "extra-compatible",
  "version": "0.1.5",
  "description": "",
  "keywords": [],
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "browser": {
    "./lib/performance-now.js": "./lib/performance-now.browser.js",
    "./lib/hmac-sha256.js": "./lib/hmac-sha256.browser.js",
    "./lib/sha256.js": "./lib/sha256.browser.js"
  },
  "sideEffects": false,
  "repository": "git@github.com:BlackGlory/extra-compatible.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src",
    "test": "jest --config jest.config.js",
    "test:coverage": "jest --coverage --config jest.config.js",
    "prepublishOnly": "run-s clean build",
    "clean": "rimraf lib",
    "build": "run-s build:*",
    "build:compile": "tsc --project tsconfig.build.json --module commonjs --target es2018 --outDir lib",
    "build:patch": "tscpaths -p tsconfig.build.json -s ./src -o ./lib",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint build test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@peculiar/webcrypto": "^1.4.1",
    "@types/jest": "^29.2.6",
    "@types/node": "16",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "husky": "4",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.1.1",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.5",
    "tscpaths": "^0.0.9",
    "tslib": "^2.4.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@blackglory/prelude": "^0.2.2",
    "iterable-operator": "^3.1.1"
  }
